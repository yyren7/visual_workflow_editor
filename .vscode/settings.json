{
    "sqltools.useNodeRuntime": true,
    "editor.fontSize": 20,
    "python.analysis.extraPaths": [
        "${workspaceFolder}"
    ],
    "cursor.general.globalCursorIgnoreList": [
        "**/.env.*",
        "**/credentials.json",
        "**/credentials.*.json",
        "**/secret.json",
        "**/secrets.json",
        "**/*.key",
        "**/*.pem",
        "**/*.pfx",
        "**/*.p12",
        "**/*.crt",
        "**/*.cer",
        "**/id_rsa",
        "**/id_dsa",
        "**/.ssh/id_*"
    ]
}