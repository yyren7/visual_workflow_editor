<?xml version="1.0" encoding="UTF-8"?>
<xml xmlns="https://developers.google.com/blockly/xml">
  <block type="connect_external_io" id="GENERATED_ID" data-blockNo="1">
    <mutation timeout="-1"></mutation>
    <!-- timeout属性存在于flow.xml示例中，具体含义需参照Contec文档或实际行为，-1可能表示无限等待或默认超时 -->
    <field name="devive_name">DIO000</field>
    <!-- devive_name (デバイス名): 操作的目标设备名称。 -->
    <!-- 有效范围: DIO000 ~ DIO005 (在 "ContecDeviceUtility" 中设置的 "Device Name") -->
    <!-- 初期値: DIO000 -->
    <!-- 示例: DIO000 -->
    <field name="external_maker">contec</field>
    <!-- external_maker (メーカー): 目标IO单元的制造商。 -->
    <!-- 有效范围: contec -->
    <!-- 初期値: contec -->
    <!-- 示例: contec -->
    <field name="io_no">1</field>
    <!-- io_no (IO 番号): 操作的目标IO编号。 -->
    <!-- 有效范围: 1 ~ 10 -->
    <!-- 初期値: 1 -->
    <!-- 示例: 1 -->
    <!-- <next> ... </next> -->
  </block>
</xml> 