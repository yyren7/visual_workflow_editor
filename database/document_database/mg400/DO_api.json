{
    "api名称": "DO（队列指令）",
    "原型": "DO(index,status)",
    "描述": "设置数字输出端口状态（队列指令）。",
    "参数": [
        {
            "参数名": "index",
            "类型": "int",
            "说明": "DO端子的编号"
        },
        {
            "参数名": "status",
            "类型": "int",
            "说明": "DO端子的状态，1：有信号；0：无信号"
        }
    ],
    "返回": "ErrorID,{},DO(index,status);",
    "示例": "DO(1,1)\n# 设置DO1为有信号。"
}